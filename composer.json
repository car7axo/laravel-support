{
  "name": "car7axo/laravel-support",
  "description": "Support files to custom Laravel Framework inspired by DDD pattern",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Diogo Brito",
      "email": "diogob.cartaxo@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Car7axo\\Laravel\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Car7axo\\Laravel\\Support\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Car7axo\\Laravel\\Support\\ServiceProvider"
      ]
    }
  }
}
